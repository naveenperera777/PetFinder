"generated":{"line":104,"column":233},"source":"Details.js","original":{"line":58,"column":6}},{"generated":{"line":104,"column":234},"source":"Details.js","original":{"line":57,"column":4}},{"generated":{"line":105,"column":0},"source":"Details.js","original":{"line":68,"column":3}},{"name":"React","generated":{"line":109,"column":2},"source":"Details.js","original":{"line":9,"column":22}},{"name":"Component","generated":{"line":109,"column":17},"source":"Details.js","original":{"line":9,"column":28}},{"name":"Details","generated":{"line":111,"column":15},"source":"Details.js","original":{"line":71,"column":15}}],"sources":{"Details.js":"import React from \"react\";\nimport pf from \"petfinder-client\";\nimport { navigate } from \"@reach/router/lib/history\";\n\nconst petfinder = pf({\n  key: process.env.API_KEY,\n  secret: process.env.API_SECRET\n});\nclass Details extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      loading: true\n    };\n  }\n\n  componentDidMount() {\n    console.log(\"id\", this.props.id);\n    petfinder.pet\n      .get({\n        output: \"full\",\n        id: this.props.animalid\n      })\n      .then(data => {\n        const pet = data.petfinder.pet;\n        let breed;\n        if (Array.isArray(pet.breeds.breed)) {\n          breed = pet.breeds.breed.join(\", \");\n        } else {\n          breed = pet.breeds.breed;\n        }\n\n        this.setState({\n          name: pet.name,\n          animal: pet.animal,\n          location: `${pet.contact.city}, ${pet.contact.state}`,\n          description: pet.description,\n          media: pet.media,\n          breed: breed,\n          loading: false\n        });\n      })\n      .catch(() => {\n        navigate(\"/\");\n      });\n  }\n\n  render() {\n    console.log(\"animal\", this.state.name); // undefined\n\n    if (this.state.loading) {\n      return <h1>loading.....</h1>;\n    }\n    const { animal, breed, location, description } = this.state;\n\n    return (\n      <div className=\"details\">\n        <div>\n          <h1>{name}</h1>\n          <h2>\n            {animal} - {breed} - {location}\n          </h2>\n          <p>{description}</p>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Details;\n"},"lineCount":null}},"hash":"c9074cceaebd9d14ca0bf86089706980","cacheData":{"env":{"API_KEY":"44fd229d3ace7ab9b6c2470e803b1e83","API_SECRET":"8ee18650aeebd58cd1f2d40d91175c7e"}}}